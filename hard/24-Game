class Solution(object):
    def judgePoint24(self, cards):
        """
        :type cards: List[int]
        :rtype: bool
        """
        EPS = 1e-6
        
        def dfs(nums):
            if len(nums) == 1:
                return abs(nums[0] - 24.0) < EPS
            
            n = len(nums)
            for i in range(n):
                for j in range(n):
                    if i == j:
                        continue
                    # build the list of remaining numbers after picking i and j
                    rest = []
                    for k in range(n):
                        if k != i and k != j:
                            rest.append(nums[k])
                    
                    a, b = nums[i], nums[j]
                    # try all possible results of a (op) b
                    candidates = [a + b, a - b, b - a, a * b]
                    if abs(b) > EPS:
                        candidates.append(a / b)
                    if abs(a) > EPS:
                        candidates.append(b / a)
                    
                    for val in candidates:
                        rest.append(val)
                        if dfs(rest):
                            return True
                        rest.pop()
            return False
        
        return dfs([float(x) for x in cards])
